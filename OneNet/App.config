<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
    </configSections>
    <connectionStrings>
        <add name="OneNet.Properties.Settings.onenetConnectionString"
            connectionString="server=localhost;user id=root;password=root;persistsecurityinfo=True;database=onenet"
            providerName="MySql.Data.MySqlClient" />
        <add name="localhost_onenet_Connection" connectionString="XpoProvider=MySql;server=localhost;port=3306;user id=root; password=root; database=onenet;persist security info=true;CharSet=utf8;SslMode=none;" />
    </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
<system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.7.9.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data></configuration>
